{
    "name": "luklewluk/backbone-todo",
    "description": "To Do list based on Backbone.js",
    "keywords": ["backbone", "todo"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lukasz Lewandowski",
            "email": "luklewluk@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "illuminate/database": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload-dev": {
        "psr-4": {
